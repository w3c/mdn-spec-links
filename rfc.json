    "rfc6454#section-7": [
        {
            "engines": [
                "blink",
                "gecko",
                "webkit"
            ],
            "filename": "http/headers/origin.json",
            "name": "Origin",
            "slug": "HTTP/Headers/Origin",
            "summary": "The Origin request header indicates where a request originates from. It doesn't include any path information. It is similar to the Referer header, but, unlike that header, it doesn't disclose the whole path.",
            "support": {
                "chrome": {
                    "version_added": true
                },
                "chrome_android": {
                    "version_added": true
                },
                "edge": {
                    "version_added": "12"
                },
                "firefox": [
                    {
                        "version_added": "70"
                    },
                    {
                        "version_added": "59",
                        "flags": [
                            {
                                "name": "network.http.sendOriginHeader",
                                "type": "preference",
                                "value_to_set": "true"
                            }
                        ],
                        "notes": "Sent with non-CORS requests since Fx59, unless they are <code>GET</code> or <code>HEAD</code>."
                    },
                    {
                        "version_added": true,
                        "partial_implementation": true,
                        "notes": "Not sent with <code>POST</code> requests until Firefox 58, see <a href='https://bugzil.la/446344'>bug 446344</a>."
                    }
                ],
                "firefox_android": [
                    {
                        "version_added": "59",
                        "flags": [
                            {
                                "name": "network.http.sendOriginHeader",
                                "type": "preference",
                                "value_to_set": "true"
                            }
                        ],
                        "notes": "Sent with non-CORS requests since Fx59, unless they are <code>GET</code> or <code>HEAD</code>."
                    },
                    {
                        "version_added": true,
                        "partial_implementation": true,
                        "notes": "Not sent with <code>POST</code> requests until Firefox 58, see <a href='https://bugzil.la/446344'>bug 446344</a>."
                    }
                ],
