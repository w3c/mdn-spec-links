            "filename": "css/properties/animation-timeline.json",
            "name": "scroll",
            "slug": "CSS/animation-timeline/scroll",
            "mdn_url": "https://developer.mozilla.org/en-US/docs/Web/CSS/animation-timeline/scroll",
            "summary": "The scroll() CSS function can be used with animation-timeline to indicate an ancestor element and scrollbar axis that will provide the anonymous scroll timeline for animating the current element.",
            "support": {
                "chrome": {
                    "version_added": false
                },
                "chrome_android": "mirror",
                "edge": "mirror",
                "firefox": {
                    "version_added": "101",
                    "notes": "Zero scroll range is treated as 100% but should be 0% (see <a href='https://bugzil.la/1780865'>bug 1780865</a>).",
                    "flags": [
                        {
                            "type": "preference",
                            "name": "layout.css.scroll-linked-animations.enabled",
                            "value_to_set": "true"
                        }
                    ]
                },
                "firefox_android": "mirror",
                "ie": {
                    "version_added": false
                },
                "oculus": "mirror",
                "opera": "mirror",
                "opera_android": "mirror",
                "safari": {
                    "version_added": false
                },
                "safari_ios": "mirror",
                "samsunginternet_android": "mirror",
                "webview_android": "mirror",
                "edge_blink": {
                    "version_added": false
                }
            },
            "title": "scroll()"
        }
    ],
